{
    "name": "livepool",
    "version": "0.4.0",
    "description": "debugging proxy for web developers base on NodeJS",
    "keywords": ["proxy", "debug", "web", "develop", "reload", "fiddler"],
    "maintainers": [{
        "name": "rehorn",
        "web": "https://github.com/rehorn"
    }],
    "homepage": "https://github.com/rehorn/livepool",
    "dependencies": {
        "colors": "*",
        "mime": "*",
        "rimraf": "*",
        "underscore": "*",
        "iconv-lite": "*",
        "express.io": "*",
        "http-proxy": "*",
        "graceful-fs": "*",
        "request": "*",
        "http-proxy-agent": "*"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/rehorn/livepool.git"
    },
    "engines": {
        "node": ">= 0.8.x"
    },
    "bugs": {
        "url": "https://github.com/rehorn/livepool/issues"
    },
    "bin": {
        "mod": "./bin/livepool"
    },
    "preferGlobal": true,
    "main": "node-webkit.html",
    "webkit": {
        "plugin": true
    },
    "window": {
        "title": "livepool ui",
        "icon": "link.png",
        "toolbar": false,
        "frame": true,
        "width": 960,
        "height": 600,
        "position": "mouse",
        "min_width": 400,
        "min_height": 200
    },
    "node-main": "./node-webkit.js"
}
